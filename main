#!/bin/bash

declare -A MYTOOLS
#PREFIX=https://raw.githubusercontent.com/Jaynarol/bash-tools/master/menu

JSON=$(curl https://api.github.com/repos/Jaynarol/bash-tools/contents/menu)

MYTOOLS[Laravel]="${PREFIX}/laravel"

PS3='Please select your tools: '
select i in "${!MYTOOLS[@]}"
do
  bash <(curl -s ${MYTOOLS[$i]})
  break
done